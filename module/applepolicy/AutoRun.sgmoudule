#!name=ApplePolicy
#!desc=Surge 苹果策略自动切换脚本

[Rule]
AND,((DOMAIN,iosapps.itunes.apple.com), (SCRIPT,SpeedLimit)),Apple

[Script]
SpeedLimit = type=rule,timeout=60,script-path=https://raw.githubusercontent.com/teaslink/surge/release/script/SpeedLimit.js,argument=Group=Apple&policy=Booster&time=15&minSpeed=15
